---
import FormConstancy from "../../../components/common/FormConstancy";
import Layout from "../../../layouts/Layout.astro";
const token = Astro.cookies.get("jwt")?.value as string;
const userId = Astro.cookies.get("id")?.value as string;

if (!token || !userId) return Astro.redirect("/login");
---

<Layout title="Crear Constancias de Registro">
 <FormConstancy type="constancia-registros" client:load/>
</Layout>
